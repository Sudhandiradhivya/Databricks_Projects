{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "TripDF"
		},
		"sink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sink'"
		},
		"source_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'source'"
		},
		"trip_transactions_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'trip_transactions'"
		},
		"sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://trip.dfs.core.windows.net/"
		},
		"source_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mysqlservertrip.database.windows.net"
		},
		"source_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mysqldatabase"
		},
		"source_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "testrole"
		},
		"trip_transactions_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://trip.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "TripDF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"sink_accountKey": {
						"value": "[parameters('sink_accountKey')]"
					},
					"source_password": {
						"value": "[parameters('source_password')]"
					},
					"trip_transactions_accountKey": {
						"value": "[parameters('trip_transactions_accountKey')]"
					},
					"sink_properties_typeProperties_url": {
						"value": "[parameters('sink_properties_typeProperties_url')]"
					},
					"source_properties_typeProperties_server": {
						"value": "[parameters('source_properties_typeProperties_server')]"
					},
					"source_properties_typeProperties_database": {
						"value": "[parameters('source_properties_typeProperties_database')]"
					},
					"source_properties_typeProperties_userName": {
						"value": "[parameters('source_properties_typeProperties_userName')]"
					},
					"trip_transactions_properties_typeProperties_url": {
						"value": "[parameters('trip_transactions_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}